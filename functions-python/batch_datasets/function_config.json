{
  "name": "batch-datasets",
  "description": "Retrieve all feeds and publish them to Pub/Sub for processing the datasets",
  "entry_point": "batch_datasets",
  "timeout": 20,
  "memory": "256Mi",
  "trigger_http": true,
  "include_folders": ["database_gen", "helpers", "dataset_service"],
  "secret_environment_variables": [
    {
      "key": "FEEDS_DATABASE_URL",
      "project_id": {
        "mobility-feeds-dev": "mobility-feeds-qa"
      }
    }
  ],
  "ingress_settings": "ALLOW_INTERNAL_AND_GCLB",
  "max_instance_request_concurrency": 20,
  "max_instance_count": 10,
  "min_instance_count": 0,
  "available_cpu": 1
}
